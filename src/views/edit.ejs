<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Create users</title>
    <link rel="stylesheet" href="/CSS/styles.css">
</head>

<body>
    <div class="topnav">
        <a href="/">Home</a>
        <a href="/create">Create users</a>
        <a href="/edit" class="active">Update Users</a>
    </div>

    <div class="form-add-new">
        <form action="/create-user" method="post">
            <fieldset>
                <legend>Update a users</legend>

                <div class="input_group">
                    <label for="">Email: </label>
                    <input type="text" name="email" value="<%= userEdit.email  %>">
                </div>

                <div class="input_group">
                    <label for="">Name: </label>
                    <input type="text" name="myName" value="<%= userEdit.name  %>">
                </div>

                <div class="input_group">
                    <label for="">City: </label>
                    <input type="text" name="city" value="<%= userEdit.city  %>">
                </div>
                <div>
                    <button>Save</button>
                </div>
            </fieldset>
        </form>
    </div>
</body>

</html>